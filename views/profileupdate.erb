<h1>PROFILE UPDATER</h1>

<form div id="updater" method="POST" action="/profile-update">

				<label for="fname">First Name:</label><br>
				<input id="name" class="input" name="fname" type="text" value="" size="30" /><br>

				<label for="lname">Last Name:</label><br>
				<input id="name" class="input" name="lname" type="text" value="" size="30" /><br>

						     
			  <label for="email">E-mail:</label><br>
			  <input id="email" class="input" name="email" type="text" value="" size="30" /><br>

			  <label for="password">Password:</label><br>
			  <input id="password" class="input" name="password" type="text" value="" size="30" /><br>

			  <label for="username">Username:</label><br>
			  <input id="usersame" class="input" name="username" type="text" value="" size="30" /><br>

			  <label for="location">Location:</label><br>
			  <input id="location" class="input" name="location" type="text" value="" size="30" /><br>

			  <label for="age">Age:</label><br>
			  <input id="age" class="input" name="age" type="integer" value="" size="4" /><br>

			  <button class="button2"><input id="submit_button" type="submit" value="Submit" /></button>
	    
		</form>